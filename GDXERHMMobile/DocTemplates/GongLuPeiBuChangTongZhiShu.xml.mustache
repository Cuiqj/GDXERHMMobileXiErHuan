        <pdf:text point="188,176">之</pdf:text>
        <pdf:paragraph rect="{19,159},{175,57}" indent="head:41" lineHeight="10"  static="NO" fontSize="9">{{ payReason }}</pdf:paragraph>
        <pdf:text point="25,183">规定，本单位依法做出如下公路赔（补）偿处理认定:</pdf:text>
