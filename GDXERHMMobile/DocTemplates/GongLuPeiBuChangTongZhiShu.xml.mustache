        <pdf:text point="45,102" maxWidth="41" static="NO" indent="head:68" lineHeight="8.5" fontSize="10">{{ caseReason }}</pdf:text>
        <pdf:text point="81,102">一案，经</pdf:text>
        <pdf:line fromPoint="100,106.5" toPoint="152,106.5" lineWidth="0.8"/>
        <pdf:text point="103,102" maxWidth="50" static="NO" indent="head:68" lineHeight="8.5" fontSize="10">{{ agencyCity }}</pdf:text>
        
        
		 
		<pdf:text point="22.5,97.5" maxWidth="50" static="NO" indent="head:68" lineHeight="8.5" fontSize="10">{{ agency }}</pdf:text>
        <pdf:text point="151.5,102">依法调查核实:</pdf:text>
		
		<pdf:text point="31,112">当事人</pdf:text>
