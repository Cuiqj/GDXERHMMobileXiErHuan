		<pdf:line fromPoint="72,201" toPoint="120,201" lineWidth="0.8"/>
		<pdf:paragraph rect="{72.5,193},{45,30}" indent="head:0;tail:0" lineHeight="4" static="NO"  fontSize="10">{{ payDetail }}</pdf:paragraph>
		<pdf:text point="120,197.5">请 在 收 到 本 通 知 书 之</pdf:text>
        <pdf:text point="30,207.5">日起15日内持本通知书到</pdf:text>
		<pdf:line fromPoint="85,213" toPoint="119,213" lineWidth="0.8"/>
		<pdf:paragraph fontSize="11.5" rect="{86.5,204},{28,40}" indent="head:0;tail:0" lineHeight="4" static="NO" fontSize="10">{{ paymentPlace }}</pdf:paragraph>
        <pdf:text point="130,207.5">获缴纳路产赔（补）偿费，</pdf:text>
		<pdf:text point="30,215">逾期不缴的，本单位特申请人民法院强制执行或依法强制执行。</pdf:text>
		<pdf:text point="22.5,224"></pdf:text>
		<pdf:text point="31.5,222">当事人对本通知书认定的事实和赔（补）偿费有疑义的，可在收到本通知书之</pdf:text>
		<pdf:text point="31,230">日起60日内，向</pdf:text>
		<pdf:line fromPoint="78,233.5" toPoint="121,233.5" lineWidth="0.8"/>
		<pdf:text point="82,230" static="NO" indent="head:68" lineHeight="8.5" fontSize="10">{{ reviewOrgan }}</pdf:text>
		<pdf:text point="120.5,230">申请复核或者向人民法院起诉。</pdf:text>
		<pdf:text point="115,272.5"> 年     月      日</pdf:text>
        <pdf:text point="104.5,272.5" static="NO" >{{year}}</pdf:text>
        <pdf:text point="124.5,272.5" static="NO">{{month}}</pdf:text>
